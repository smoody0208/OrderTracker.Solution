@{
  Layout = "_Layout";
}
<div class="container">
  <h1>Order Details: </h1>

  <p>Type: @Model["order"].TypeOfBread</p>
  <p>Quantity: @Model["order"].Quantity</p>
  <p>Date: @Model["order"].Date</p>
  <p>Price: $@Model["order"].Price</p>

  <button class="btn">Back to:<a href='/vendors/@Model["vendor"].Id'>@Model["vendor"].VendorName Orders</a></button>
  <br>
</div>  